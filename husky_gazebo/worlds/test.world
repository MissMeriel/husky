<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- ground plane -->
<!--    <include>
      <uri>model://ground_plane</uri>
    </include>
-->

    <model name="box">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
		<box>
		<size>1 1 1</size>
		</box>
          </geometry>
      <surface>
        <friction>
          <ode>
            <mu>100</mu>
            <mu2>100</mu2>
          </ode>
        </friction>
      </surface>
        </collision>

      </link>
    </model>

  </world>
</sdf>
